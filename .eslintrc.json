{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        
        "quotes": ["error", "single"],
        "semi" : ["error", "always"],
        "indent":["error", 2], 
        "no-multi-spaces": ["error"]
    }
}
